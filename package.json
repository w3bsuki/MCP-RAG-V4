{
  "name": "mcp-rag-dev-system",
  "version": "1.0.0",
  "description": "A production-ready 3-agent Claude Code system that converts PRDs into deployable applications through parallel development in isolated git worktrees, coordinated via shared documents and enhanced with persistent RAG memory.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch .mcp/enhanced-server.ts",
    "start": "node dist/.mcp/enhanced-server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup-agents": "tsx scripts/setup-agents.ts",
    "sync-worktrees": "tsx scripts/sync-worktrees.ts",
    "health-check": "tsx scripts/health-check.ts",
    "monitor": "tsx scripts/coordination-monitor.ts",
    "smart-system": "npm run monitor & npm run dev",
    "full-system": "npm run setup-agents && npm run smart-system"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@types/node": "^20.19.1",
    "@zilliz/milvus2-sdk-node": "^2.5.11",
    "ai": "^4.3.16",
    "axios": "^1.10.0",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "node-cron": "^4.1.0",
    "node-notifier": "^10.0.1",
    "simple-git": "^3.28.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "ws": "^8.18.2",
    "zod": "^3.25.65"
  }
}
