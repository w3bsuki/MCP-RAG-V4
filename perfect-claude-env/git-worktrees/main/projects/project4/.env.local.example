# Anthropic API Key for AI predictions
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_api_key_here

# Optional: Customize the model (defaults to claude-3-haiku)
# ANTHROPIC_MODEL=claude-3-opus-20240229

# Stripe Configuration
# Get these from: https://dashboard.stripe.com/
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Stripe Product Price IDs (create these in Stripe Dashboard)
STRIPE_PRO_PRICE_ID=price_pro_monthly_1999
STRIPE_TEAM_PRICE_ID=price_team_monthly_4999

# Stripe Webhook Secret (for webhook endpoint)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3003